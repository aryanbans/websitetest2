{
  "name": "wysa-website-demo1",
  "description": "A blank template to get started with Payload",
  "version": "1.0.0",
  "main": "dist/server.js",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts nodemon",
    "build:payload": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload build",
    "build:server": "tsc",
    "build": "yarn copyfiles && yarn build:payload && yarn build:server",
    "serve": "cross-env PAYLOAD_CONFIG_PATH=dist/payload.config.js NODE_ENV=production node dist/server.js",
    "copyfiles": "copyfiles -u 1 \"src/**/*.{html,css,scss,ttf,woff,woff2,eot,svg,jpg,png}\" dist/",
    "generate:types": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload generate:types",
    "generate:graphQLSchema": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload generate:graphQLSchema",
    "payload": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload"
  },
  "dependencies": {
    "@babel/register": "^7.12.10",
    "@faceless-ui/css-grid": "^1.0.1",
    "@faceless-ui/modal": "^1.1.4",
    "@faceless-ui/mouse-info": "^1.2.4",
    "@faceless-ui/scroll-info": "^1.2.5",
    "@faceless-ui/window-info": "^1.2.7",
    "@payloadcms/bundler-webpack": "^1.0.7",
    "@payloadcms/db-mongodb": "^1.7.2",
    "@payloadcms/plugin-cloud": "^3.0.1",
    "@payloadcms/richtext-slate": "^1.5.2",
    "color": "^3.1.3",
    "create-payload-app": "^1.0.0",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jss-global": "^3.0.0",
    "next": "^13.4.12",
    "nodemon": "^3.1.4",
    "payload": "^2.27.0",
    "react": "^18.2.0",
    "react-jss": "^10.10.0",
    "react-toastify": "^9.1.2",
    "react-transition-group": "^4.4.5",
    "typescript": "^5.1.6"
  },
  "devDependencies": {
    "@trbl/eslint-config": "^2.0.3",
    "@types/express": "^4.17.11",
    "@types/html-validator": "^5.0.0",
    "@types/node": "^20.5.1",
    "@types/react": "^18.2.21",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "eslint": "^8.48.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jest": "^27.3.1",
    "eslint-plugin-jest-dom": "^4.0.2",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "jest": "^29.6.1",
    "lint-staged": "^13.2.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  }
}
